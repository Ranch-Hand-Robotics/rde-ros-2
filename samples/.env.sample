# Sample environment file for ROS 2 debugging
# This demonstrates the envFile feature

# ROS 2 Domain ID - change this to isolate your ROS 2 network
ROS_DOMAIN_ID=42

# Enable localhost only mode for security
ROS_LOCALHOST_ONLY=1

# Custom log level
RCUTILS_CONSOLE_OUTPUT_FORMAT=[{severity}] [{name}]: {message}

# Example custom variables
MY_ROBOT_NAME=test_robot
DEBUG_MODE=1
