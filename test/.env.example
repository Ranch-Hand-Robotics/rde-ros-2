# Example .env file for ROS 2 launch configurations
# This file shows how to define environment variables that can be loaded
# using the envFile property in launch.json

# ROS 2 specific variables
ROS_DOMAIN_ID=42
ROS_LOCALHOST_ONLY=1

# Custom application variables
APP_LOG_LEVEL=DEBUG
APP_CONFIG_PATH=/path/to/config

# Variables with quotes
MESSAGE="Hello ROS 2"
PATH_WITH_SPACES="/path with spaces/to/something"

# Variables with special characters
CONNECTION_STRING=key=value;port=5432

# Empty variables are also supported
OPTIONAL_VAR=
